<div id="filters-{{ paginator.id }}">
	<div class="row">
		{% for f in filters %}
			<div class="col-sm-3" style="margin-bottom: 10px;">
				{{ form_widget(f, {"attr": {"class": "form-control upper"} } ) }}
			</div>
		{% endfor %}
		<div class="col-sm-3">
			<button class="btn btn-primary" id="search-{{ paginator.id }}" type="button" title="Search">
	            Search
	        </button>
	        <button class="btn btn-default" id="clean-{{ paginator.id }}" type="button" title="Clean">
	            Clean
	        </button>
		</div>
	</div>
</div>
